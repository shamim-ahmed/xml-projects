            </forecast-period>
            <forecast-period index="2" start-time-local="2010-08-08T00:00:00+10:00" end-time-local="2010-08-09T00:00:00+10:00" start-time-utc="2010-08-07T14:00:00Z" end-time-utc="2010-08-08T14:00:00Z">
